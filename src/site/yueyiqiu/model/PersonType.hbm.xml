<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="site.yueyiqiu.model.PersonType" table="tb_persontype">
        <id name="typeId" type="int" access="field">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="typeName" type="java.lang.String" access="field">
            <column name="type" />
        </property>
    </class>
</hibernate-mapping>
